{
  "name": "system-migrate",
  "version": "0.1.0",
  "tasks": {
    "test": "deno run --allow-all tests/helpers/run.ts system-migrate-test --build",
    "run:cmd": "deno run --allow-run tests/helpers/run.ts system-migrate-test --build",
    "check": "deno check **/*.ts",
    "fmt": "deno fmt",
    "dev": "deno run --watch -A mod.ts"
  },
  "imports": {
    "$tests/": "./tests/",
    "$commands/": "./commands/",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/cli": "jsr:@std/cli@^1.0.12",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.11"
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 100,
    "semiColons": true
  },
  "compilerOptions": {
    "strict": true,
    "lib": ["deno.window"],
    "noImplicitAny": true
  }
}
